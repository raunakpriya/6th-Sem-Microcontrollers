#include "msp430.h"
ORG 0x3010
DW 0x0004
ORG 0x3020
DW 0x000F
NAME main
ORG 0FFFEh
DC16 init
RSEG CSTACK
RSEG CODE
init: MOV #SFE(STACK),SP
main: NOP
MOV.W #WDTPW+WDTHOLD,&WDTCTL
MOV.W #0004H,0x3000
MOV.W #000FH,0x3002
CLR.W R4
CLR.W R7
CLR.W R8
CLR.W R11
MOV.W 0X3010(R4), R5
MOV.W 0X3020(R4),R6
UP:  CMP R6,R5
JL Label
SUB.W R6,R5
INC.W R7
JMP UP
LABEL: MOV.W R7,0X3030(R4)  ; QUOTIENT
MOV.W R5,0X3040(R4) ;   REMAINDER
JMP $
END
